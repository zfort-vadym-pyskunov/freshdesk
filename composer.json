{
    "name": "kuznetsov-zfort/freshdesk",
    "license": "MIT",
    "keywords": ["laravel", "freshdesk"],
    "authors": [
        {
            "name": "Gary Kuznetsov",
            "email": "kuznetsov@zfort.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/auth": "5.7.* | 5.8.*",
        "illuminate/config": "5.7.* | 5.8.*",
        "illuminate/support": "5.7.* | 5.8.*",
        "zfort-vadym-pyskunov/php-enum": "^1.0.2"
    },
    "autoload": {
        "psr-4": {
            "KuznetsovZfort\\Freshdesk\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zfort-vadym-pyskunov/php-enum.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KuznetsovZfort\\Freshdesk\\FreshdeskServiceProvider"
            ],
            "aliases": {
                "Freshdesk": "KuznetsovZfort\\Freshdesk\\Freshdesk"
            }
        }
    },
    "prefer-stable": true
}
